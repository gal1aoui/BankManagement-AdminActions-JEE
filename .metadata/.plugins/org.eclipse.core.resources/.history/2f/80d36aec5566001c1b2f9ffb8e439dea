package compte;

import java.util.Date;

import users.Client;

public class CompteEpargne extends Compte {
	
	private float taux;

	public float getTaux() {
		return taux;
	}

	public void setTaux(float taux) {
		this.taux = taux;
	}

	public CompteEpargne() {
		super();
		// TODO Auto-generated constructor stub
	}

	public CompteEpargne(float solde, Date createdAt, Client client, float taux) {
		super(solde, createdAt, client);
		this.taux = taux;
	}
}
